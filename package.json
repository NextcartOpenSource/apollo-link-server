{
  "name": "apollo-link-server",
  "version": "1.0.0",
  "description": "Use an Apollo Server instance to request data.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "npm run format:base -- --write",
    "format:base": "prettier --config prettierrc.js src/{**/*,*}.ts package.json tsconfig.json eslintrc.js prettierrc.js jestrc.js",
    "format:check": "npm run format:base -- --list-different",
    "lint": "npm run lint:base -- --fix",
    "lint:base": "eslint --config eslintrc.js src/{**/*,*}.ts eslintrc.js prettierrc.js jestrc.js",
    "lint:check": "npm run lint:base",
    "test": "jest --config jestrc.js",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stuart4/apollo-link-server.git"
  },
  "bugs": {
    "url": "https://github.com/stuart4/apollo-link-server/issues"
  },
  "homepage": "https://github.com/stuart4/apollo-link-server#readme",
  "keywords": [
    "backend",
    "apollo",
    "link",
    "server",
    "graphql"
  ],
  "author": "Jacob Stuart <stuart4@nextcart.com>",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-server-micro": "^2.9.16",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "graphql-tag": "^2.10.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "apollo-link": "^1.2.13",
    "graphql": "^14.5.8"
  }
}
